{
  "name": "@cfwest/api",
  "description": "Api Module and Models / Classes for CrossFire West",
  "version": "0.2.6",
  "private": false,
  "license": "MIT",
  "author": {
    "name": "Patrick Jung",
    "email": "patrick.pddstudio@gmail.com"
  },
  "homepage": "https://github.com/cfna/cfwest-api",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfna/cfwest-api.git"
  },
  "directories": {
    "lib": "./dist",
    "test": "./tests"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "crossfire",
    "west",
    "cfwest",
    "z8games",
    "cf",
    "rest"
  ],
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc",
    "clean": "rimraf dist/",
    "lint": "tslint --project tsconfig.json --config tslint.json --fix",
    "pretest": "yarn build",
    "test": "jest",
    "format": "prettier --write \"**/*.ts\"",
    "prerelease": "yarn build",
    "prerelease:patch": "yarn build",
    "prerelease:minor": "yarn build",
    "prerelease:major": "yarn build",
    "release": "dotenv release-it --",
    "release:patch": "dotenv release-it --increment=patch",
    "release:minor": "dotenv release-it --increment=minor",
    "release:major": "dotenv release-it --increment=major",
    "prepackage:create": "yarn build",
    "package:create": "yarn pack --filename $PKG_ARCHIVE_NAME",
    "docs:dev": "docz dev",
    "predocs:build": "rimraf .docz/dist",
    "docs:build": "docz build",
    "predocs:api": "rimraf .docz/dist-api",
    "docs:api": "typedoc"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "lodash.merge": "^4.6.2",
    "puppeteer": "^1.13.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^11.9.5",
    "@types/puppeteer": "^1.12.1",
    "@types/vfile-message": "^2.0.0",
    "@types/winston": "^2.4.4",
    "conventional-changelog-cli": "^2.0.12",
    "docz": "^0.13.7",
    "docz-theme-default": "^0.13.7",
    "dotenv-cli": "^3.1.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "release-it": "^10.3.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.13.0",
    "tslint-sonarts": "^1.9.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3333"
  }
}