    "name": "cfna"
  },
  "homepage": "https://github.com/cfna/cfwest-api",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfna/cfwest-api.git"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "nx run-many --target=build",
