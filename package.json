    "eslint": "^8.23.1",
    "fliegdoc": "^0.5.2",
    "husky": "^8.0.1",
    "jest": "^28.0.0",
    "lint-staged": "^13.0.3",
    "nx": "14.7.6",
    "prettier": "^2.7.1",
    "release-it": "^15.6.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.13",
